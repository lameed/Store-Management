<div id="submenu">
  <h4 class="title">All Consumables</h4>
</div>
<!-- <div class="link_add large-12 columns">
  <%= link_to 'Add Consumables', new_consumable_path, :class => 'btn sign_in button left' %>
</div> -->
<table class="large-12 columns">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumables.each do |consumable| %>
      <tr>
        <td><%= link_to consumable.id, consumable %></td>
        <td><%= link_to consumable.name, consumable %></td>
        <td><%= consumable.description %></td>
        <td><%= link_to 'Full Details', consumable, :class => 'btn sign_in button' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


