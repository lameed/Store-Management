<h5> Your requests</h5>

<table>
	<% for item in @part_request.part_line_items %>
	<tr>
    <td><%= item.quantity %> &times; <%= item.part.title %></td>
 </tr>
 <% end %>
</table>
<%= button_to "Make Request" , new_order_url, :method=>:get, :class => 'btn sign_in button right' %>
<%= button_to 'Empty Request', @request, :method => :delete, :confirm => 'Are you sure?', :class => 'btn sign_in button left' %>